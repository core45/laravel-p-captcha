{
    "name": "core45/laravel-p-captcha",
    "description": "Advanced CAPTCHA system for Laravel applications with beam alignment, pattern matching, and computational challenges",
    "keywords": ["laravel", "captcha", "security", "proton", "beam-alignment"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kornel",
            "email": "kornel@core45.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Core45\\LaravelPCaptcha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Core45\\LaravelPCaptcha\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Core45\\LaravelPCaptcha\\Providers\\PCaptchaServiceProvider"
            ],
            "aliases": {
                "PCaptcha": "Core45\\LaravelPCaptcha\\Facades\\PCaptcha"
            }
        }
    },
    "minimum-stability": "dev",
    "version": "0.0.15"
}
